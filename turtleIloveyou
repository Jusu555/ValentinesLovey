from turtle import Turtle, done

t = Turtle()

t.pensize(5)

t.color("white")
t.back(450)
t.color("black")

t.left(90)
t.forward(100)
t.back(100)
t.right(90)
t.color("white")
t.forward(100)
t.left(90)
t.color("black")
t.forward(100)
t.back(100)
t.right(90)
